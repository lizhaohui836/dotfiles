case $OSTYPE in
    darwin14.0)
            PATH=/Users/feng/hbase-0.96.1.1-hadoop2/bin
            export PATH=~/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/X11/bin:~/bin/go/bin:/usr/local/sbin:$PATH
            export JAVA_HOME=/Library/Java/JavaVirtualMachines/jdk1.8.0_25.jdk/Contents/Home
            export LC_ALL=en_US.UTF-8
            export GOROOT=~/bin/go
            ;;
    linux-gnu)
            unset JAVA_HOME
            export JAVA_HOME=/usr/lib/jvm/java-1.8.0-openjdk-amd64
            export ANT_HOME=/opt/apache-ant-1.8.3
            export M2_HOME=~/bin/maven
            export M2=$M2_HOME/bin
            export LC_ALL=en_US.UTF-8
            export GOROOT=~/bin/go
            PATH=/usr/local/bin:/usr/bin:/bin:/usr/local/games:/sbin:/usr/sbin:/opt/scala-2.10.3/bin
            PATH=~/bin:~/eclipse:$JAVA_HOME/bin:$PATH:$M2:$ANT_HOME/bin:$GOROOT/bin
            PATH=$PATH:/home/feng/android/android-sdk-linux/tools:/home/feng/android/android-sdk-linux/platform-tools
            export GTK_IM_MODULE=ibus
            export XMODIFIERS=@im=ibus
            export QT_IM_MODULE=ibus
            # ulimit -n 10240
            ;;
esac

